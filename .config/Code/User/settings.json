{
    "editor.fontFamily": "'JetBrains Mono', 'Droid Sans Mono', 'monospace', 'Droid Sans Fallback'",
    "editor.fontSize": 16,
    "editor.fontLigatures": true,
    "editor.rulers": [120],
    "editor.bracketPairColorization.enabled": true,
    "terminal.integrated.copyOnSelection": true,
    "files.trimTrailingWhitespace": true,
    "workbench.editor.showTabs": true,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 6,
    "vim.handleKeys": {
        "<C-f>": false
    },
    "workbench.colorTheme": "Atom Material Theme",
    "workbench.colorCustomizations": {
        "editorIndentGuide.background": "#2b3b3f",
        "editorIndentGuide.activeBackground": "#39494a",
        "list.inactiveFocusBackground": "#1a237e",
        "list.focusBackground": "#0d47a1",
        "list.activeSelectionBackground": "#455a64",
        "list.inactiveSelectionBackground": "#37474f"
    },
    "workbench.activityBar.visible": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.editor.enablePreview": false,
    "workbench.settings.useSplitJSON": true,
    "update.mode": "none",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["g", "r"],
            "commands": [
                {
                    "command": "editor.action.referenceSearch.trigger"
                }
            ]
        }
    ],
    "terminal.integrated.fontSize": 15,
    "terminal.integrated.lineHeight": 1.25,
    "vscode_custom_css.imports": ["file:///home/max/.config/Code/User/custom-css/index.css"],
    "merge-conflict.codeLens.enabled": false,
    "editor.parameterHints.enabled": false,
    "breadcrumbs.enabled": true,
    "workbench.settings.editor": "json",
    "advancedNewFile.exclude": {
        "ui/node_modules": true,
        "node_modules": true
    },
    "html.format.wrapAttributes": "force-expand-multiline",
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
    "editor.minimap.enabled": false,
    "material-icon-theme.folders.theme": "classic",
    "xmlTools.enableXmlTreeViewCursorSync": true,
    "xmlTools.splitAttributesOnFormat": true,
    "explorer.autoReveal": true,
    "workbench.list.keyboardNavigation": "simple",
    "terminal.integrated.commandsToSkipShell": ["-workbench.action.quickOpen"],
    "typescript.preferences.quoteStyle": "single",
    "cSpell.language": "en,ru",
    "cSpell.dictionaryDefinitions": [
        {
            "name": "dev-home",
            "path": "/home/max/.vscode-dict.cspell.txt"
        }
    ],
    "cSpell.dictionaries": ["dev-work", "dev-home"],
    "workbench.tree.renderIndentGuides": "always",
    "workbench.tree.indent": 16,
    "editor.suggestSelection": "first",
    "telemetry.telemetryLevel": "off",
    "qub-xml-vscode.telemetry.enabled": false,
    "prettier.jsxSingleQuote": true,
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "prettier.printWidth": 100,
    "typescript.tsserver.log": "off",
    "editor.linkedEditing": true,
    "editor.semanticHighlighting.enabled": true,
    "i18n-ally.sortKeys": true,
    "i18n-ally.annotations": false,
    "i18n-ally.keystyle": "flat",
    "i18n-ally.preferredDelimiter": ".",
    "i18n-ally.review.enabled": false,
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[code-snippets]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "emojisense.emojiDecoratorsEnabled": false,
    "editor.renderWhitespace": "trailing",
    "explorer.confirmDelete": false,
    "python.languageServer": "Pylance",
    "python.formatting.provider": "black",
    "cSpell.enableFiletypes": ["dockerfile", "shellscript", "dotenv"],
    "emojisense.unicodeCompletionsEnabled": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "yaml.customTags": ["!encrypted/pkcs1-oaep scalar", "!vault scalar"],
    "redhat.telemetry.enabled": false,
    "emmet.excludeLanguages": ["markdown", "typescriptreact"],
    "editor.tabSize": 8,
    "editor.cursorStyle": "line",
    "editor.insertSpaces": false,
    "editor.lineNumbers": "on",
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "editor.wordWrap": "off",
    "yaml.schemas": {
        "file:///home/max/.vscode/extensions/atlassian.atlascode-2.10.0/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "shellcheck.enableQuickFix": true,
    "shellcheck.exclude": ["SC1091"]
}
