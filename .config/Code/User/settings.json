{
    "editor.fontFamily": "'JetBrains Mono', 'Droid Sans Mono', 'monospace', 'Droid Sans Fallback'",
    "editor.fontSize": 15,
    "editor.fontLigatures": true,
    "editor.rulers": [120],
    "editor.renderIndentGuides": true,
    "terminal.integrated.copyOnSelection": true,
    "files.trimTrailingWhitespace": true,
    "workbench.editor.showTabs": true,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 6,
    "window.zoomLevel": 0,
    "vim.handleKeys": {
        "<C-f>": false
    },
    "workbench.colorTheme": "Atom Material Theme",
    "workbench.colorCustomizations": {
        "editorIndentGuide.background": "#2b3b3f",
        "editorIndentGuide.activeBackground": "#39494a",
        "list.inactiveFocusBackground": "#1a237e",
        "list.focusBackground": "#0d47a1",
        "list.activeSelectionBackground": "#455a64",
        "list.inactiveSelectionBackground": "#37474f"
    },
    "workbench.activityBar.visible": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.editor.enablePreview": false,
    "workbench.settings.useSplitJSON": true,
    "update.mode": "none",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["g", "r"],
            "commands": [
                {
                    "command": "editor.action.referenceSearch.trigger"
                }
            ]
        }
    ],
    "terminal.integrated.fontSize": 15,
    "terminal.integrated.lineHeight": 1.25,
    "typescript.preferences.importModuleSpecifier": "auto",
    "vscode_custom_css.imports": ["file:///home/max/.config/Code/User/custom-css/index.css"],
    "vscode_custom_css.policy": true,
    "merge-conflict.codeLens.enabled": false,
    "editor.parameterHints.enabled": false,
    "breadcrumbs.enabled": true,
    "workbench.settings.editor": "json",
    "advancedNewFile.exclude": {
        "ui/node_modules": true,
        "node_modules": true
    },
    "workbench.iconTheme": "material-icon-theme",
    "html.format.wrapAttributes": "force-expand-multiline",
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
    "editor.minimap.enabled": false,
    "material-icon-theme.folders.theme": "classic",
    "xmlTools.enableXmlTreeViewCursorSync": true,
    "xmlTools.splitAttributesOnFormat": true,
    "explorer.autoReveal": true,
    "workbench.list.keyboardNavigation": "simple",
    "terminal.integrated.commandsToSkipShell": ["-workbench.action.quickOpen"],
    "typescript.preferences.quoteStyle": "single",
    "cSpell.language": "en,ru",
    "cSpell.dictionaryDefinitions": [
        {
            "name": "dev-home",
            "path": "/home/max/.vscode-dict.cspell.txt"
        }
    ],
    "cSpell.dictionaries": ["dev-work", "dev-home"],
    "tslint.alwaysShowRuleFailuresAsWarnings": false,
    "cSpell.userWords": [
        "Cacheble",
        "Geocoded",
        "Interop",
        "Lifecycle",
        "NETPING",
        "Parens",
        "Platonova",
        "Snackbar",
        "Synesis",
        "Timepicker",
        "Unmount",
        "Uploader",
        "WAKEUP",
        "WIALON",
        "ZINDEX",
        "arraybuffer",
        "bundlesize",
        "camelcase",
        "chmod",
        "codecov",
        "codeframe",
        "commitlint",
        "docx",
        "dotfiles",
        "downlevel",
        "esnext",
        "fdescribe",
        "fdescribee",
        "forin",
        "frontmatter",
        "gcloud",
        "gitignore",
        "grayscale",
        "iban",
        "jscpd",
        "junit",
        "latlng",
        "lcov",
        "minmax",
        "mkdir",
        "monospace",
        "ngcc",
        "ohmyzsh",
        "polylines",
        "println",
        "repos",
        "rodmax",
        "scrollwheel",
        "styleext",
        "tabindex",
        "vapp",
        "visualstudio",
        "xlink",
        "yagni"
